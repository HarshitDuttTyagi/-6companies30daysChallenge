class Solution{
public:
    vector<int> findTwoElement(vector<int> arr, int n) {
        // code here
        int a,b;
        
    //Repeating element
for(int i=0;i<n;i++){
if(arr[abs (arr[i])-1]<0){
a = abs (arr[i]);
}
else{
  arr[abs(arr[i])-1] = -arr[abs(arr[i])-1];
    }
 }

//Missing element
for(int i=0;i<n;i++){
if(arr[i]>0) {
b = i+1;
break;
}
}
vector<int> v;
v.push_back(a);
v.push_back(b);
return v;
    }
};

//in time O(n)
//and space O(1)
